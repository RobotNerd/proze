{
  "name": "proze-compiler",
  "version": "0.1.0",
  "description": "Compile proze to target document type.",
  "main": "build/index.js",
  "scripts": {
    "antlr4ts": "antlr4ts -visitor ../grammar/Proze.g4 -o generated && mv ./grammar/* ./generated && rm -fr ./grammar",
    "build-all": "npm run antlr4ts && npx tsc",
    "build": "npx tsc",
    "compile": "node build/index.js",
    "test": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobotNerd/proze.git"
  },
  "author": "Marshall Bowles",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RobotNerd/proze/issues"
  },
  "homepage": "https://github.com/RobotNerd/proze#readme",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.7.14",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  }
}
