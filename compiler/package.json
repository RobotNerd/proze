{
  "name": "proze-compiler",
  "version": "0.1.0",
  "description": "Compile proze to target document type.",
  "main": "dist/src/index.js",
  "scripts": {
    "antlr4ts": "antlr4ts -visitor ../grammar/Proze.g4 -o generated -Xexact-output-dir",
    "build-all": "npm run antlr4ts && npm run build",
    "build": "npx tsc --project ./tsconfig.build.json",
    "compile": "node ./dist/src/index.js",
    "test": "jest -c ./jest.unit.json",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --coverage -c ./jest.unit.json",
    "test:coverage:watch": "npm run test:coverage -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobotNerd/proze.git"
  },
  "author": "Marshall Bowles",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RobotNerd/proze/issues"
  },
  "homepage": "https://github.com/RobotNerd/proze#readme",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.16",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "jest": "^29.0.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  }
}
